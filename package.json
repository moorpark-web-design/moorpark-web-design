{
  "name": "ventura-coast-web-design",
  "version": "1.0.0",
  "description": "Ventura Coast Web Design website",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run scss && eleventy --serve",
    "build": "npm run scss && eleventy",
    "scss": "sass assets/styles/scss/main.scss assets/styles/main.css --no-source-map --style=expanded",
    "scss:watch": "sass assets/styles/scss/main.scss assets/styles/main.css --no-source-map --style=expanded --watch"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "sass": "^1.89.2"
  }
}
